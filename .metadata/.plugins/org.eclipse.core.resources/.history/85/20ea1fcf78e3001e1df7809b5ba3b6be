package com.sathya.springex.Formersrole;

public class Former {
	private Work work;
	private HardWork hardWork;
	public Work getWork() {
		return work;
	}
	public void setWork(Work work) {
		this.work = work;
	}
	public HardWork getHardWork() {
		return hardWork;
	}
	public void setHardWork(HardWork hardWork) {
		this.hardWork = hardWork;
	}
	public void FormerInfo() {
		work.WorkInfo();
		hardWork.HardWorkInfo();
	}
	

}
